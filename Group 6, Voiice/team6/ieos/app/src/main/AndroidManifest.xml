<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hubert.ieos" >
    <uses-sdk android:minSdkVersion="11" />
    <uses-permission android:name="android.permission.INTERNET" /><uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme" >
        //android:theme="@style/AppTheme" >
    <activity
        android:name=".MainActivity"
        android:label="@string/app_name"
        android:logo="@drawable/raspi"
        >

        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

    <activity android:name=".LoginActivity"
        android:label="@string/login">
    </activity>

    <activity android:name=".TaaDActivity"
        android:label="@string/speechrecognition">
    </activity>

    <activity android:name=".SpeechActivity"
        android:label="@string/speechrecognition">
    </activity>

    <activity android:name=".StreamingActivity"
        android:label="@string/streaming">
    </activity>
</application>
</manifest>
